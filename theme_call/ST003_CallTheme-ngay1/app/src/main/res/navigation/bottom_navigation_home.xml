<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/bottom_navigation_home"
    app:startDestination="@id/themesFragment">
    <fragment
        android:id="@+id/themesFragment"
        android:name="com.namseox.st003_calltheme.ui.themes.ThemesFragment"
        android:label="ThemesFragment" >
        <action
            android:id="@+id/action_themesFragment_to_diyThemeFragment"
            app:destination="@id/diyThemeFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
    </fragment>
    <fragment
        android:id="@+id/diyThemeFragment"
        android:name="com.namseox.st003_calltheme.ui.diy_theme.DiyThemeFragment"
        android:label="DiyThemeFragment" >
        <action
            android:id="@+id/action_diyThemeFragment_to_myDesignFragment"
            app:destination="@id/myDesignFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
    </fragment>
    <fragment
        android:id="@+id/myDesignFragment"
        android:name="com.namseox.st003_calltheme.ui.my_design.MyDesignFragment"
        android:label="MyDesignFragment" >
        <action
            android:id="@+id/action_myDesignFragment_to_settingFragment"
            app:destination="@id/settingFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
    </fragment>
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.namseox.st003_calltheme.ui.setting.SettingFragment"
        android:label="SettingFragment" />
</navigation>